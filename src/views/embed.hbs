<div id="powerbi-container">
    <iframe
        title="Power BI Report"
        width="100%"
        height="750"
        src={{urlDashboard}}
        frameborder="0"
        allowFullScreen="true">
    </iframe>
</div>
<div class="container mt-4">
    <h2 class="mb-3">Información Básica</h2>
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-body">
                  <div class="mb-3">
                      <h5 class="card-subtitle mb-2 text-muted">Título:</h5>
                      <p class="card-text">{{title}}</p>
                  </div>
                   <div class="mb-3">
                      <h5 class="card-subtitle mb-2 text-muted">Descripción:</h5>
                      <p class="card-text">{{description}}</p>
                   </div>
                </div>
            </div>
        </div>
    </div>
</div>